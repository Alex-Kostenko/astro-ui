{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@style/*": ["src/style/*"],
      "@i18n/*": ["src/i18n/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
      "@service/*": ["src/service/*"],
      "@helpers/*": ["src/helpers/*"],
      "@constant/*": ["src/constant/*"],
      "@interfaces/*": ["src/interfaces/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
